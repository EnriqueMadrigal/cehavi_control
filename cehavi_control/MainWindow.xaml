<Window x:Class="cehavi_control.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cehavi_control"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Sistema">
                <MenuItem Header="Respaldar" Click="mnuNew_Click" />
                <Separator />
                <MenuItem Header="_Exit" Click="mnuNew_Click2"/>
            </MenuItem>
            <MenuItem Header="_Paciente">
                <MenuItem Header="_Nuevo" />
                <MenuItem Header="_Consultar" Click="listaPacientes" />
                <MenuItem Header="_Bajas" />
            </MenuItem>
        </Menu>
        <TextBox AcceptsReturn="True" />
    </DockPanel>
</Window>
